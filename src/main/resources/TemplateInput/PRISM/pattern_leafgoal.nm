$DEC_HEADER$
//$CONST_PARAM$ double R_$GID$=0.99; //testing reliability
$CONST_PARAM$ double R_$GID$=1; //testing cost

module $MODULE_NAME$
	s$GID$ :[0..4] init 0;
	
	$DEC_TYPE$
	[] s$GID$ =  1 -> R_$GID$ : (s$GID$'=2) + (1 - R_$GID$) : (s$GID$'=4);//running to final state
	[next$TIME_SLOT$] s$GID$ = 2 -> (s$GID$'=2);//final state success
	[next$TIME_SLOT$] s$GID$ = 3 -> (s$GID$'=3);//final state skipped
	[next$TIME_SLOT$] s$GID$ = 4 -> (s$GID$'=4);//final state failure
endmodule
